generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model user {
  id       Int    @id @default(autoincrement())
  username String
  email    String @unique
  password String
}

model admin {
  id       Int    @id @default(autoincrement())
  username String
  email    String @unique
  password String
}

model engineer {
  id       Int    @id @default(autoincrement())
  username String
  email    String @unique
  password String
}

model technician {
  id       Int    @id @default(autoincrement())
  username String
  email    String @unique
  password String
}


